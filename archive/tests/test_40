 UNWIND [0] AS unwind_var MATCH () WHERE [ ] <> NULL OR FALSE >= NULL OR TRUE <= 0 UNWIND [0, 3] AS unwind_var MATCH path_0 = () DETACH DELETE CHEMICALDECREASESEXPRESSION , BiologicalProcess DETACH DELETE GENEREGULATESGENE , BodyPart , DISEASELOCALIZESTOANATOMY DETACH DELETE GENEINPATHWAY MERGE path_1 = () ON CREATE SET path_0.prop9 = NULL, path_0.prop7 = NULL ON MATCH SET CHEMICALINCREASESEXPRESSION.prop2 = 'LIT0', DISEASEASSOCIATESWITHDISEASE.prop1 = NULL, Drug.prop4 = NULL, BODYPARTOVEREXPRESSESGENE.prop5 = TRUE SET DrugClass.prop2 = FALSE SET DISEASEASSOCIATESWITHDISEASE.prop9 = [ ] RETURN CellularComponent ORDER BY 'LIT2' SKIP 0 