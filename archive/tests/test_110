 UNWIND [0] AS unwind_var MATCH ()-[]->()  SET DRUGINCLASS.prop1 = Gene.prop8, BiologicalProcess.prop6 = [ ] MERGE () ON MATCH SET GENEPARTICIPATESINBIOLOGICALPROCESS.prop6 = FALSE SET CellularComponent.prop2 = GENECOVARIESWITHGENE.prop3 WITH DISTINCT GENEINPATHWAY  var ORDER BY DISEASEASSOCIATESWITHDISEASE.prop2, GENEINTERACTSWITHGENE WHERE FALSE >= [ 'LIT7' ] MERGE ()<-[:reltype6]-()-[:reltype9]->() CREATE path_0 = () RETURN * 